                    <gmd:title>
                        <gco:CharacterString>Underway data from R/V Point Sur in the Mississippi Bight, March-April 2016</gco:CharacterString>
                    </gmd:title>
                    <gmd:alternateTitle>
                        <gco:CharacterString />
                    </gmd:alternateTitle>
